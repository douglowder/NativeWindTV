{
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "expo": "~51.0.37",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "nativewind": "4.1.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@latest",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@react-native-tvos/config-tv": "^0.0.10",
    "@types/react": "~18.2.79",
    "@types/tailwindcss": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "3.3.2",
    "typescript": "~5.3.3"
  },
  "name": "nativewindtv",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "scripts": {
    "prebuild": "DEBUG=expo:* EXPO_TV=1 expo prebuild --clean",
    "start:nativewind": "tailwindcss -i ribbon.css --postcss postcss.config.js --watch",
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:web": "expo export --platform web",
    "deploy:web": "eas deploy --environment=development",
    "lint": "tailwindcss -i ribbon.css --postcss postcss.config.js; expo lint"
  }
}
